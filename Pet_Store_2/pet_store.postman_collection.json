				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"var status_code = pm.response.code\r",
							"\r",
							"console.log(status_code);\r",
							"\r",
							"if(status_code == 200){\r",
							"    var responseData = pm.response.json()\r",
							"\r",
							"    pm.test(\"Varify Order ID.\", function(){\r",
							"        pm.expect(pm.environment.get(\"ID\")).to.eql(responseData.id)\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Pet ID.\", function(){\r",
							"        pm.expect(pm.environment.get(\"Pet_ID\")).to.eql(responseData.category.id)\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Catagory name.\", function(){\r",
							"    pm.expect(pm.environment.get(\"Catagory_name\")).to.eql(responseData.category.name)\r",
							"    })\r",
							"//\r",
							"    pm.test(\"Varify Pet Name.\", function(){\r",
							"        pm.expect(pm.environment.get(\"Pet_name\")).to.eql(responseData.name)\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Catagory name.\", function(){\r",
							"    pm.expect(pm.environment.get(\"Photo_Url\")).to.eql(responseData.photoUrls[0])\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Pet Tag ID.\", function(){\r",
							"        pm.expect(pm.environment.get(\"Tag_ID\")).to.eql(responseData.tags[0].id)\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Pet Tag name.\", function(){\r",
							"    pm.expect(pm.environment.get(\"Tag_Name\")).to.eql(responseData.tags[0].name)\r",
							"    })\r",
							"\r",
							"    pm.test(\"Varify Status.\", function(){\r",
							"    pm.expect(pm.environment.get(\"Status\")).to.eql(responseData.status)\r",
							"    })\r",
							"}\r",
							"else if(status_code == 404){\r",
							"    pm.test(\"ID expired\")\r",
							"}\r",
							"else{\r",
							"    pm.test(\"Something went wrong.\")\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
